              "nifi_web_http_host",
              "nifi_web_http_port",
              "nifi_web_http_port_forwarding",
              "nifi_web_http_network_interface_default",
              "nifi_web_https_host",
              "nifi_web_https_port",
              "nifi_web_https_port_forwarding",
              "nifi_web_https_network_interface_default",
