              {
                "key": "class",
                "value": "org.apache.nifi.kerberos.KerberosProvider"
              },
              {
                "key": "Authentication Expiration",
                "value": "${login_identity_providers_kerberos_authentication_expiration} hours"
              },
              {
                "key": "cdh.login.identity.providers.type",
                "value": "provider"
              }
            ]
          },
          {
